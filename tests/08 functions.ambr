def f(a):
	return a - 3
end

println f(4)
println f(5) * f(6) - f(7) * f(8)

def g(a, b):
	return f(a) * f(b)
end

println g(5, 6) - g(7, 8)

def h(c):
	x = 3 + c
	return g(x, c)
end

println h(6)
println h(7)

def f(a):
	if a <= 2:
		result = 1
	else:
		result = f(a - 1) + f(a - 2)
	end
	return result
end

println f(12)